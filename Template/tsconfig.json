{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "outDir": "src/scripts",
    "rootDir": "src/scripts",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/scripts/**/*"],
  "exclude": ["node_modules"]
}
